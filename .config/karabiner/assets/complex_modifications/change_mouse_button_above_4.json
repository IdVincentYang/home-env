{
  "title": "Change Mac Mouse [4: Super Key], [5:CMD_s]",
  "rules": [
    {
      "description": "Mouse 4 Switch Super Key",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button4"
          },
          "to": [
            {
              "key_code": "right_command",
              "modifiers": [
                "left_control",
                "left_option",
                "left_shift"
              ]
            }
          ],
          "to_if_alone": [
            {
              "pointing_button": "button4"
            }
          ]
        }
      ]
    },
    {
      "description": "Mouse 5 Switch CMD_s",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button5"
          },
          "to": [
            {
              "key_code": "s",
              "modifiers": [
                "left_gui"
              ]
            }
          ]
        }
      ]
    }
  ]
}